{
  "name": "aave-watcher",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-only",
  "type": "module",
  "dependencies": {
    "@bgd-labs/aave-address-book": "^4.32.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@ethersproject/bytes": "^5.8.0",
    "@ethersproject/providers": "^5.8.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "bignumber.js": "^9.1.2",
    "ethers": "^6.13.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.9.3",
    "viem": "^2.23.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && zip -r chrome-extension.zip dist/* -x '*.DS_Store'",
    "preview": "vite preview",
    "build:firefox": "NODE_ENV=firefox vite build && cp manifest_firefox.json firefox_dist/manifest.json && web-ext build --source-dir firefox_dist --overwrite-dest && zip -r firefox-extension.zip dist/ -x '*.DS_Store'",
    "compile": "zip -r sourcecode.zip src public -x '*.pem' -x '*node_modules*' -x '*.git*' -x '*web-ext-artifacts*' -x '*.DS_Store'",
    "publish:firefox": "npx web-ext sign  --api-key=$API_KEY --api-secret=$API_SECRET  -s firefox_dist --channel $CHANNEL"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.306",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/webextension-polyfill": "^0.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "prettier": "3.6.2",
    "vite": "^6.0.3",
    "vite-plugin-web-extension": "^4.1.1",
    "web-ext": "^8.4.0"
  }
}
